  27          
  28          /* spi_write_then_read - write 8-bits to the SPI to clock out and then 
  29           * return the clocked in data.
  30           * @byte: data write to SPI
  31           * @ret: clocked in data after clock out 8-bits
  32           */
  33          char spi_write_then_read(char byte)
  34          {
  35   1              EXIF &= ~SPI_READY;     /* clean the SPI done flag before write */
  36   1              SPI_DATA = byte;        /* send 8 bits */
  37   1              while (!(EXIF & SPI_READY))     /* wait until done */
  38   1                      ;
  39   1              return SPI_DATA;        /* return data */
  40   1      }
